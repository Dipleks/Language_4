-- таблица заданий PS
CREATE TABLE IF NOT EXISTS exercises_ps
(id SERIAL PRIMARY KEY, english VARCHAR(200), russia VARCHAR(200));
-- таблица заданий to be
CREATE TABLE IF NOT EXISTS exercises_to_be
(id SERIAL PRIMARY KEY, english VARCHAR(200), russia VARCHAR(200));
-- таблица экзаменов PS
CREATE TABLE IF NOT EXISTS exams_ps
(id SERIAL PRIMARY KEY, english VARCHAR(200), russia VARCHAR(200));
-- таблица экзаменов to be
CREATE TABLE IF NOT EXISTS exams_to_be
(id SERIAL PRIMARY KEY, english VARCHAR(200), russia VARCHAR(200));
-- таблица слов
CREATE TABLE IF NOT EXISTS words
(id SERIAL PRIMARY KEY, english VARCHAR(60), russia VARCHAR(60));
-- таблица текстов
CREATE TABLE IF NOT EXISTS texts
(id SERIAL PRIMARY KEY, english TEXT, russia TEXT);